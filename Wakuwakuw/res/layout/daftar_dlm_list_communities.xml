<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
	android:id="@+id/relativeLayout2"
	android:layout_width="fill_parent"
	android:layout_height="fill_parent"
	android:background="@drawable/listview_selector_color"
	android:padding="5dip">

    <ImageView
    	android:layout_width="40dip"
    	android:layout_height="40dip"
    	android:id="@+id/imgViewLogoCommun"
    	android:src="@drawable/icon"
    	android:layout_alignParentLeft="true"
    	android:layout_centerInParent="true"
    	android:layout_marginRight="5dip">
    </ImageView>

    <TextView
    	android:layout_height="wrap_content"
    	android:text="TextView"
    	android:textSize="14sp"
    	android:textStyle="bold"
    	android:textColor="#000000"
    	android:layout_width="wrap_content"
    	android:id="@+id/txtViewNamaCommun"
    	android:layout_toRightOf="@+id/imgViewLogoCommun"
    	android:layout_marginLeft="2dip">
    </TextView>
    
    <TextView
        android:layout_width="wrap_content"
    	android:layout_height="wrap_content"
    	android:text="TextView"
    	android:textSize="12sp"
    	android:textColor="#000000"
    	android:id="@+id/txtViewJenisCommun"
    	android:layout_toRightOf="@+id/imgViewLogoCommun"
    	android:layout_below="@+id/txtViewNamaCommun"
    	android:layout_margin="2dip">
    </TextView>

    <TextView
    	android:layout_height="wrap_content"
    	android:text="TextView"
    	android:textSize="12sp"
    	android:textColor="#000000"
    	android:layout_width="wrap_content"
    	android:id="@+id/txtViewDeskripsiCommun"
    	android:layout_toRightOf="@+id/imgViewLogoCommun"
    	android:layout_below="@+id/txtViewJenisCommun"
    	android:layout_marginLeft="2dip"
    	android:visibility="gone">
    </TextView>
    
    <TextView
        android:layout_width="wrap_content"
    	android:layout_height="wrap_content"
    	android:text="id"
    	android:textColor="#000000"
    	android:textSize="8sp"
    	android:id="@+id/txtViewIdCommun"
    	android:layout_toRightOf="@+id/imgViewLogoCommun"
    	android:layout_below="@+id/txtViewDeskripsiCommun"
    	android:layout_margin="2dip"
    	android:visibility="gone">
    </TextView>
    
    
    
    
    <ImageView
    	android:layout_width="15dip"
    	android:layout_height="15dip"
    	android:id="@+id/imgViewLogoCommunMember"
    	android:src="@drawable/members_icon"
    	android:layout_toRightOf="@+id/imgViewLogoCommun"
    	android:layout_below="@+id/txtViewDeskripsiCommun"
    	android:layout_marginTop="5dip">
    </ImageView>
    
    <TextView
        android:layout_width="wrap_content"
    	android:layout_height="wrap_content"
    	android:text="2"
    	android:textSize="10sp"
    	android:textColor="#000000"
    	android:id="@+id/txtViewJumlahCommunMember"
    	android:layout_below="@+id/txtViewDeskripsiCommun"
    	android:layout_toRightOf="@+id/imgViewLogoCommunMember"
    	android:layout_marginLeft="3dip"
    	android:layout_marginRight="15dip"
    	android:layout_marginTop="5dip">
    </TextView>
    
    <ImageView
    	android:layout_width="15dip"
    	android:layout_height="15dip"
    	android:id="@+id/imgViewLogoCommunComment"
    	android:src="@drawable/comments_icon"
    	android:layout_toRightOf="@+id/txtViewJumlahCommunMember"
    	android:layout_below="@+id/txtViewDeskripsiCommun"
    	android:layout_marginTop="5dip">
    </ImageView>
    
    <TextView
        android:layout_width="wrap_content"
    	android:layout_height="wrap_content"
    	android:text="2"
    	android:textSize="10sp"
    	android:textColor="#000000"
    	android:id="@+id/txtViewJumlahCommunComment"
    	android:layout_below="@+id/txtViewDeskripsiCommun"
    	android:layout_toRightOf="@+id/imgViewLogoCommunComment"
    	android:layout_marginLeft="3dip"
    	android:layout_marginRight="15dip"
    	android:layout_marginTop="5dip">
    </TextView>
    
    <ImageView
    	android:layout_width="15dip"
    	android:layout_height="15dip"
    	android:id="@+id/imgViewLogoCommunLike"
    	android:src="@drawable/love_icon"
    	android:layout_toRightOf="@+id/txtViewJumlahCommunComment"
    	android:layout_below="@+id/txtViewDeskripsiCommun"
    	android:layout_marginTop="5dip">
    </ImageView>
    
    <TextView
        android:layout_width="wrap_content"
    	android:layout_height="wrap_content"
    	android:text="2"
    	android:textSize="10sp"
    	android:textColor="#000000"
    	android:id="@+id/txtViewJumlahCommunLike"
    	android:layout_toRightOf="@+id/imgViewLogoCommunLike"
    	android:layout_below="@+id/txtViewDeskripsiCommun"
    	android:layout_marginLeft="3dip"
    	android:layout_marginTop="5dip">
    </TextView>

</RelativeLayout>